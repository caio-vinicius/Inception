server {
    listen 443 ssl default_server;
    listen [::]:443 ssl default_server;
    ssl_certificate /etc/nginx/certs/cert.pem;
    ssl_certificate_key /etc/nginx/certs/key.pem;
    ssl_protocols TLSv1.3;
    root /var/www/html;
    index index.nginx-debian.html;
    server_name _;
}
server {
    listen 80;
    listen [::]:80;
    return 301 https://$host$request_uri;
}
